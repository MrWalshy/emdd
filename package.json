{
  "name": "emdd",
  "version": "0.10.0",
  "description": "",
  "main": "emdd.js",
  "type": "module",
  "homepage": "https://emdd.morganwalsh.dev",
  "scripts": {
    "start": "node ./emdd.js",
    "deploy": "npm test&& npm publish",
    "start:site": "npm run build:site&& ws --directory ./documentation/build --rewrite \"/emdd -> /\" --rewrite \"/emdd/(.*) -> /$1\"",
    "build:site": "npm start -- site --config=./documentation/site_config.json",
    "deploy:site": "npm run build:site&& cp CNAME ./documentation/build/CNAME&& gh-pages -d documentation/build",
    "test": "node --experimental-vm-modules ./node_modules/jest/bin/jest --coverage",
    "test:watch": "npm run test -- --watchAll --verbose",
    "mess": "node ./examples/mess.js"
  },
  "bin": {
    "emdd": "./emdd.js"
  },
  "keywords": [],
  "author": "MrWalshy",
  "repository": {
    "type": "git",
    "url": "https://github.com/MrWalshy/emdd.git"
  },
  "license": "MIT",
  "dependencies": {
    "rehype-format": "^4.0.1",
    "rehype-katex": "^6.0.2",
    "rehype-stringify": "^9.0.3",
    "remark-gfm": "^3.0.1",
    "remark-parse": "^10.0.1",
    "remark-rehype": "^10.1.0",
    "remark-toc": "^8.0.1",
    "unified": "^10.1.2",
    "unified-stream": "^2.0.0"
  },
  "devDependencies": {
    "@types/jest": "^29.2.6",
    "gh-pages": "^5.0.0",
    "jest": "^29.3.1",
    "local-web-server": "^5.2.1"
  }
}
